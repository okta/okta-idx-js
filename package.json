{
  "name": "@okta/okta-idx-js",
  "version": "0.8.2",
  "description": "Okta JS-based consumption layer for IDX API",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist && rimraf test-reports",
    "banners": "node scripts/maintain-banners.js",
    "build": "yarn build:prod",
    "build:dev": "yarn bundle:dev",
    "build:prod": "node scripts/build.js",
    "bundle": "yarn bundle:prod",
    "bundle:dev": "webpack --mode=development",
    "bundle:prod": "webpack --mode=production",
    "lint": "eslint src",
    "lint-report": "eslint -f checkstyle -o test-reports/lint/idx-lint-result.xml src",
    "prepublishOnly": "yarn build",
    "test:unit": "jest test/unit",
    "test-report": "jest test/unit --coverage --ci",
    "test:integration": "jest test/integration",
    "test": "yarn lint && yarn test:unit"
  },
  "repository": "https://github.com/okta/okta-idx-js.git",
  "keywords": [],
  "license": "Apache-2.0",
  "private": true,
  "bugs": {
    "url": "https://github.com/okta/okta-idx-js/issues"
  },
  "homepage": "https://github.com/okta/okta-idx-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.7.1",
    "@peculiar/webcrypto": "^1.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "chalk": "^4.1.0",
    "core-js": "^3.8.0",
    "cross-fetch": "^3.0.4",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "globby": "^11.0.1",
    "jest": "^24.9.0",
    "jest-junit": "^12.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@babel/runtime-corejs3": "^7.12.5",
    "jsonpath": "^1.0.2"
  }
}
